<div class="col-md-12 row project-list">
  <div class="project-category col-md-12">    
    
    <ul class="nav nav-pills">
      <li class="active"> <a href="#">POPULAR</a> </li>
      <li> <a href="#">NEW</a></li>
      <li> <a href="#">MOST $ RAISED</a></li>
      <li> <a href="#">MOST VOTED</a></li>
      <li> <a href="#">MOST COMMENTED</a></li>
    </ul>
  
  </div>
  <h3 class="page-header">
    Search results for "<%= @keyword %>"...
  </h3>
  <% @projects.each do |project| %>
    <div class="project-block col-md-3">
      <%= render template:'projects/item', :locals=>{ project: project} %>
    </div>
  <% end %>

</div>
<div class="col-md-12">
  <div class="project-category col-md-12">     
    <div class="row project-block col-md-6">
      <button class="btn btn-default btn-prev pull-right"> Prev </button>
    </div>
    <div class="project-block col-md-6">
      <div class="project-block col-md-6">
        <button class="btn btn-default btn-next"> Prev </button>
      </div>
    </div>
  </div>
</div>
  
